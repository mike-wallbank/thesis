@article{MINOSCollaboration2014,
abstract = {We report on a new analysis of neutrino oscillations in MINOS using the complete set of accelerator and atmospheric data. The analysis combines the {\$}\backslashnu{\_}{\{}\backslashmu{\}}{\$} disappearance and {\$}\backslashnu{\_}{\{}e{\}}{\$} appearance data using the three-flavor formalism. We measure {\$}|\backslashDelta m{\^{}}{\{}2{\}}{\_}{\{}32{\}}|=[2.28-2.46]\backslashtimes10{\^{}}{\{}-3{\}}\backslashmbox{\{}\backslash,eV{\}}{\^{}}{\{}2{\}}{\$} (68{\%} C.L.) and {\$}\backslashsin{\^{}}{\{}2{\}}\backslashtheta{\_}{\{}23{\}}=0.35-0.65{\$} (90{\%} C.L.) in the normal hierarchy, and {\$}|\backslashDelta m{\^{}}{\{}2{\}}{\_}{\{}32{\}}|=[2.32-2.53]\backslashtimes10{\^{}}{\{}-3{\}}\backslashmbox{\{}\backslash,eV{\}}{\^{}}{\{}2{\}}{\$} (68{\%} C.L.) and {\$}\backslashsin{\^{}}{\{}2{\}}\backslashtheta{\_}{\{}23{\}}=0.34-0.67{\$} (90{\%} C.L.) in the inverted hierarchy. The data also constrain {\$}\backslashdelta{\_}{\{}CP{\}}{\$}, the {\$}\backslashtheta{\_}{\{}23{\}}{\$} octant degeneracy and the mass hierarchy; we disfavor 36{\%} (11{\%}) of this three-parameter space at 68{\%} (90{\%}) C.L.},
