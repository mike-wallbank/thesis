keywords = {Liquid argon,Neutrino oscillations},
pages = {347--350},
publisher = {Elsevier B.V.},
title = {{Liquid Argon Neutrino Detector Development at Fermilab}},
url = {http://dx.doi.org/10.1016/j.nuclphysbps.2012.09.054},
volume = {229-232},
year = {2012}
