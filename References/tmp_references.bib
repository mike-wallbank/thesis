@article{LArElectronLifetime,
abstract = {A materials test system (MTS) has been developed at FNAL to assess the suitability of materials for use in a large liquid argon time projection chamber. During development of the MTS, it was noted that controlling the cryostat pressure with a 'raining' condenser reduced the electron drift lifetime in the liquid argon. The effect of condensing has been investigated using a series of passive materials to filter the condensate. We report the results of these studies and of tests on different candidate materials for detector construction. The inferred reduction of electron drift lifetime by water concentrations in the parts per trillion is of particular interest. {\textcopyright} 2009 Elsevier B.V.},
author = {Andrews, R. and Jaskierny, W. and J{\"{o}}stlein, H. and Kendziora, C. and Pordes, S. and Tope, T.},
doi = {10.1016/j.nima.2009.07.024},
file = {:Users/mike/Library/Application Support/Mendeley Desktop/Downloaded/Andrews et al. - 2009 - A system to test the effect of materials on electron drift lifetime in liquid argon and the effect of water.pdf:pdf},
issn = {01689002},
journal = {Nuclear Instruments and Methods in Physics Research, Section A: Accelerators, Spectrometers, Detectors and Associated Equipment},
keywords = {Electron drift lifetime,LArTPC,Liquid argon,Purity,Water},
number = {2},
pages = {251--258},
title = {{A system to test the effect of materials on electron drift lifetime in liquid argon and the effect of water}},
volume = {608},
year = {2009}
}
