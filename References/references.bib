@inproceedings{CRY2007,
abstract = {The CRY software library generates correlated cosmic-ray particle shower distributions at one of three elevations (sea level, 2100 m, and 11300 m) for use as input to transport and detector simulation codes. Our simulation is based on precomputed input tables derived from full MCNPX simulations of primary cosmic rays on the atmosphere and benchmarked against published cosmic-ray measurements. Our simulation provides all particle production (muons, neutrons, protons, electrons, photons, and pions) with the proper flux within a user-specified area and altitude. The code generates individual showers of secondary particles sampling the energy, time of arrival, zenith angle, and multiplicity with basic correlations, and has user controls for latitude (geomagnetic cutoff) and solar cycle effects. We provide a function library, callable from C, C++, and Fortran, and interfaces to popular Monte Carlo transport codes (MCNP, MCNPX, COG, Geant4). The software library and examples can be downloaded from http://nuclear.llnl.gov/simulation.},
author = {Hagmann, C and Lange, D and Wright, D},
booktitle = {2007 IEEE Nuclear Science Symposium Conference Record},
doi = {10.1109/NSSMIC.2007.4437209},
file = {:Users/mike/GoogleDrive/PhD/Literature/LArSoft/CosmicRayShowerGeneratorCRYForMonteCarloTransportCodes.pdf:pdf},
issn = {1082-3654},
keywords = {Atmosphere,Atmospheric measurements,Atmospheric modeling,C,C language,C++,C++ language,COG,Cosmic rays,Detectors,FORTRAN,Fortran,Geant4,MCNP,MCNPX simulations,Mesons,Monte Carlo methods,Monte Carlo transport codes,Sea level,Sea measurements,Software libraries,astronomy computing,cosmic ray showers,cosmic-ray particle shower distributions,cosmic-ray shower generator,electron production,gamma-ray production,muon production,neutron production,photon production,pion production,primary cosmic rays,proton production,sea level,secondary particles,solar cycle effects,zenith angle},
month = {oct},
pages = {1143--1146},
title = {{Cosmic-ray shower generator (CRY) for Monte Carlo transport codes}},
volume = {2},
year = {2007}
}
