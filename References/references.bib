doi = {10.1016/0370-2693(92)91521-A},
file = {:Users/mike/Library/Application Support/Mendeley Desktop/Downloaded/Anselmann et al. - 1992 - Solar neutrinos observed by GALLEX at Gran Sasso.pdf:pdf},
