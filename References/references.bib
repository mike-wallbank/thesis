@article{MINOSCollaboration2014,
abstract = {We report on a new analysis of neutrino oscillations in MINOS using the complete set of accelerator and atmospheric data. The analysis combines the $\nu_{\mu}$ disappearance and $\nu_{e}$ appearance data using the three-flavor formalism. We measure $|\Delta m^{2}_{32}|=[2.28-2.46]\times10^{-3}\mbox{\,eV}^{2}$ (68% C.L.) and $\sin^{2}\theta_{23}=0.35-0.65$ (90% C.L.) in the normal hierarchy, and $|\Delta m^{2}_{32}|=[2.32-2.53]\times10^{-3}\mbox{\,eV}^{2}$ (68% C.L.) and $\sin^{2}\theta_{23}=0.34-0.67$ (90% C.L.) in the inverted hierarchy. The data also constrain $\delta_{CP}$, the $\theta_{23}$ octant degeneracy and the mass hierarchy; we disfavor 36% (11%) of this three-parameter space at 68% (90%) C.L.},
