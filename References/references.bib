@article{Bahcall2005,
abstract = {We construct solar models with the newly calculated radiative opacities from the Opacity Project (OP) and\nwith recently determined (lower) heavy-element abundances. We compare the results from the new models with the predictions of a series of models that use OPAL radiative opacities, older determinations of the surface heavyelement abundances, and refinements of nuclear reaction rates. For all the variations we consider, solar models that are constructed with the newer and lower heavy-element abundances advocated by Asplund et al. disagree by much more than the estimated measuring errors with the helioseismological determinations of the depth of the solar convective zone, the surface helium composition, the internal sound speeds, and the density profile. Using the new OP radiative opacities, the ratio of the 8\nB neutrino flux calculated with the older and larger heavyelement abundances (or with the newer and lower heavy-element abundances) to the total neutrino flux measured by the Sudbury Neutrino Observatory is 1.09 (0.87) with a 9% experimental uncertainty and a 16% theoretical uncertainty, 1 j errors.},
archivePrefix = {arXiv},
arxivId = {astro-ph/0412440},
