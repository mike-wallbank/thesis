@article{35tonCameras2017,
abstract = {The prevalent use of large volume liquid argon detectors strongly motivates the development of novel readout and monitoring technology which functions at cryogenic temperatures. This paper presents the development of a cryogenic CMOS camera system suitable for use inside a large volume liquid argon detector for online monitoring purposes. The characterisation of the system is described in detail. The reliability of such a camera system has been demonstrated over several months, and recent data from operation within the liquid argon region of the DUNE 35 t cryostat is presented. The cameras were used to monitor for high voltage breakdown inside the cryostat, with capability to observe breakdown of a liquid argon time projection chamber in situ. They were also used for detector monitoring, especially of components during cooldown.},
author = {McConkey, N and Spooner, N and Thiesse, M and Wallbank, M and Warburton, T K},
doi = {10.1088/1748-0221/12/03/P03014},
file = {:Users/mike/Library/Application Support/Mendeley Desktop/Downloaded/McConkey et al. - 2017 - Cryogenic CMOS cameras for high voltage monitoring in liquid argon.pdf:pdf},
journal = {Journal of Instrumentation},
number = {03},
pages = {P03014},
title = {{Cryogenic CMOS cameras for high voltage monitoring in liquid argon}},
url = {http://stacks.iop.org/1748-0221/12/i=03/a=P03014},
volume = {12},
year = {2017}
}
@article{Hirata1988,
