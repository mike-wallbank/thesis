@article{Aarnio1989,
abstract = {First measurements of the mass and width of the Z0 performed at the newly commissioned LEP Collider by the DELPHI Collaboration are presented. The measuements are derived from the study of multihadronic final states produced in e+e− annihilations at several energies around the Z0 mass. The values found for the mass and width are M(Z0)=91.06±0.09 (stat) ±0.045 (syst.) GeV and $\Gamma$(Z0)=2.42±0.21 (stat.) GeV respectively, froma three-parameter fit to the line shape. A two-parameter fit in the framework of the standard model yields for the number of light neutrino species N$\nu$=2.4±0.4 (stat.) ±0.5 (syst.).},
