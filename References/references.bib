@article{Huber2011,
abstract = {We investigate the quantitative impact that data from the second oscillation maximum has on the performance of wide band beam neutrino oscillation experiments. We present results for the physics sensitivities to standard three flavor oscillation, as well as results for the sensitivity to non-standard interactions. The quantitative study is performed using an experimental setup similar to the Fermilab to DUSEL Long Baseline Neutrino Experiment (LBNE). We find that, with the single exception of sensitivity to the mass hierarchy, the second maximum plays only a marginal role due to the experimental difficulties to obtain a statistically significant and sufficiently background-free event sample at low energies. This conclusion is valid for both water {{\v{C}}}erenkov and liquid argon detectors. Moreover, we confirm that non-standard neutrino interactions are very hard to distinguish experimentally from standard three-flavor effects and can lead to a considerable loss of sensitivity to $\theta$ 13, the mass hierarchy and CP violation.},
author = {Huber, Patrick and Kopp, Joachim},
doi = {10.1007/JHEP03(2011)013},
file = {:Users/mike/Library/Application Support/Mendeley Desktop/Downloaded/Huber, Kopp - 2011 - Two experiments for the price of one The role of the second oscillation maximum in long baseline neutrino experimen.pdf:pdf},
issn = {1029-8479},
journal = {Journal of High Energy Physics},
number = {3},
pages = {13},
title = {{Two experiments for the price of one? The role of the second oscillation maximum in long baseline neutrino experiments}},
url = {http://dx.doi.org/10.1007/JHEP03(2011)013},
volume = {2011},
year = {2011}
}
